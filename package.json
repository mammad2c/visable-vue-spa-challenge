{
  "name": "visable-vue-spa-challenge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "yarn": "<=v1.22.19",
    "node": ">= 12.x.x",
    "npm": "please use yarn to run this project"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "vite --port 3000",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier ./**/*.{vue,ts,js,css,scss,md,json} --write",
    "lint": "eslint ./**/*.{vue,ts,js} --fix",
    "lint-staged": "lint-staged",
    "test:unit": "vitest --environment jsdom --root src/"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node": "^18.13.0",
    "primevue": "^3",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^21.1.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.6",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vitest": "^0.28.4",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "*.{vue,ts,js}": [
      "eslint --fix"
    ],
    "*.{vue,ts,js,css,scss,md,json}": "prettier --write"
  }
}
