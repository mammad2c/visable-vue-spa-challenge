{
  "name": "visable-vue-spa-challenge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "yarn": "<=v1.22.19",
    "node": ">= 12.x.x",
    "npm": "please use yarn to run this project"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier ./**/*.{ts,js,css,scss,md,json} --write",
    "lint": "eslint ./**/*.{ts,js} --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node": "^18.13.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,jsx,css,scss,md,json}": "prettier --write"
  }
}
